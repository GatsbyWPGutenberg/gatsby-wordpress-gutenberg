{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "path",
      "url": "/plugins/wp-gatsby-gutenberg",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "wp-graphql/wp-graphql": "0.9.1",
    "gatsbyjs/wp-gatsby": "0.4.1",
    "pristas-peter/wp-graphql-gutenberg": "0.3.4",
    "pristas-peter/wp-graphql-gutenberg-acf": "0.3.0",
    "gatsby-wordpress-gutenberg/wp-gatsby-gutenberg": "@dev"
  }
}
