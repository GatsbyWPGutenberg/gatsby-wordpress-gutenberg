{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "path",
      "url": "/gatsby-wordpress-gutenberg",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "gatsby-wordpress-gutenberg/gatsby-wordpress-gutenberg": "@dev"
  }
}
