{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "path",
      "url": "/plugins/wp-gatsby-gutenberg",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "/plugins/wp-gatsby",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "gatsbyjs/wp-gatsby": "@dev",
    "gatsby-wordpress-gutenberg/wp-gatsby-gutenberg": "@dev",
    "wp-api/basic-auth": "dev-master"
  }
}
